<core:FragmentDefinition
        xmlns:core="sap.ui.core"
        xmlns:l="sap.ui.layout"
        xmlns:f="sap.ui.layout.form"
        xmlns="sap.m">
    <Dialog
            title="{i18n>ipd.title}"
            icon="sap-icon://filter"
            contentWidth="650px"
            class="sapUiNoContentPadding">
        <content>
            <f:SimpleForm 
                id="siemensUiInputParamsForm"
                layout="ResponsiveGridLayout"
                labelSpanL="5"
                labelSpanM="5"
                columnsL="1"
                columnsM="1" />
        </content>
        <beginButton>
            <Button text="{i18n>ipd.btn.submit}" press="onInputParameterSearch" />
        </beginButton>
        <endButton>
            <Button text="{i18n>ipd.btn.cancel}" press="onInputParameterCancel" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
